{
  "moduleFileExtensions": [
    "js",
    "jsx",
    "ts",
    "tsx",
    "json"
  ],
  "setupFiles": [
    "raf/polyfill",
    "<rootDir>/test/setup.js"
  ],
  "modulePathIgnorePatterns": [
    "<rootDir>/cypress/"
  ],
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  }
}